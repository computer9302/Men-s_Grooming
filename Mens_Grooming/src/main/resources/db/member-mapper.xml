<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
	PUBLIC "-//mybatis.org//DTD mapper 3.5//EN"
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
	
<mapper namespace="mymember">

	<insert id="insert" parameterType="MemberDto">
		INSERT INTO STSMEMBER
		VALUES(#{member_id}, #{pass_word}, #{pass_word2}, #{address}, #{phone_number}, #{e_mail})
	</insert>
	
	<insert id="insertSns" parameterType="Member">
		INSERT INTO OAUTH_USER_DETAILS
		VALUES(#{name},#{password}, #{e_mail})
	</insert>

	<select id="select" parameterType="Member">
	SELECT * FROM OAUTH_USER_DETAILS
	</select>

</mapper>